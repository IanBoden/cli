{% for common_param in common_params_definition.params %}
{% if common_param.name not in param_manual_setting_list %}
{% if common_param.name in params_source_target_list %}
- name: {{ common_param.name }}
  value: $(params.target_{{ common_param.name }})
{% else %}
- name: {{ common_param.name }}
  value: $(params.{{ common_param.name }})
{% endif %}
{% endif %}
{% endfor %}

{% for pre_upgrade_param in preupgrade_params_definition.params %}
{% if pre_upgrade_param.name not in param_manual_setting_list %}
{% if pre_upgrade_param.name in params_source_target_list %}
- name: {{ pre_upgrade_param.name }}
  value: $(params.target_{{ pre_upgrade_param.name }})
{% else %}
- name: {{ pre_upgrade_param.name }}
  value: $(params.{{ pre_upgrade_param.name }})
{% endif %}
{% endif %}
{% endfor %}
